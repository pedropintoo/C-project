elipse : Ellipse at (-650, 0) with {
    length = (200, 135);
    fill = "black";
}

view : View with {
    Ox = 30;
    Oy = 0;
    width = 1800;
    height = 401;
    title = "1";
    background = "alice blue";
}


refresh view;

for i in 0..20 do {
    rotate elipse by 35;
    refresh view after 1s;
}



print "Press any mouse button to quit";

p : Point = wait mouse click; # instantiate a Point and waits for a mouse click
print p;

close view;