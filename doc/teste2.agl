view : View with {
    width = 601;
    height = 601;
    title = "Illustrating the minimum level graphical models";
    background = "alice blue";
}

line: Arc at (100,56) with {
    length = (50,25);
    start = 0;
    extent = 90;
    outline = "tomato";
}

refresh view;

for i in 1..5 do {
    move line by (45,45); 
    refresh view after 1 s;
}

